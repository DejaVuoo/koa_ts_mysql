#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules/jest/bin/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules/jest/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules/jest/bin/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules/jest/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/registry.npmmirror.com+jest@29.7.0_@types+node@20.10.4_ts-node@10.9.1/node_modules:/mnt/d/WORK/GIT/koa_ts_mysql/koa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
